{
    "latex-workshop.latex.recipes": [{
        "name": "xelatex",
        "toolsb": ["xelatex", "biber", "xelatex", "xelatex"],
        "tools": ["xelatex"]
    }],
    "latex-workshop.latex.tools": [{
        "name": "xelatex",
        "command": "xelatex",
        "args": [
            "-synctex=1",
            "-interaction=nonstopmode",
            "-file-line-error",
            "-output-directory=out",
            "%DOC%"
        ]
    }, {
        "name": "biber",
        "command": "biber",
        "args": [
            "--output_directory=out",
            "%DOCFILE%"
        ]
    }],
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.chktex.enabled": true,
    "latex-workshop.latex.outDir": "%DIR%/out",
    "cSpell.words": [
        "Neurorobotics",
        "Plecher",
        "Sandro",
        "UBII",
        "breaklines",
        "captionpos",
        "checktex",
        "chktex",
        "darkgray",
        "discretize",
        "extendedchars",
        "hidelinks",
        "keywordstyle",
        "lstlistings",
        "mathpazo",
        "morecomment",
        "morestring",
        "ndkeywords",
        "ndkeywordstyle",
        "numbersep",
        "numberstyle",
        "pgfplots",
        "showspaces",
        "showstringspaces",
        "tabsize"
    ],
}