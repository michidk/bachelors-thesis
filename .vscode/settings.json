{
  "latex-workshop.latex.recipes": [{
    "name": "xelatex",
    "tools": ["xelatex", "biber", "xelatex", "xelatex"],
    //"tools": ["xelatex"]
  }],
  "latex-workshop.latex.tools": [{
    "name": "xelatex",
    "command": "xelatex",
    "args": [
      "-synctex=1",
      "-interaction=nonstopmode",
      "-file-line-error",
      "-output-directory=out",
      "%DOC%"
    ]
  }, {
    "name": "biber",
    "command": "biber",
    "args": [
      "--output_directory=out",
      "%DOCFILE%"
    ]
  }],
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.chktex.enabled": true,
  "latex-workshop.latex.outDir": "%DIR%/out",
  "editor.wordWrap": "on",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "files.exclude": {
    "**/.*": true,
    "out/": true
  },
  "cSpell.words": [
    "Dyrda",
    "Neurorobotics",
    "Plecher",
    "Protobuf",
    "QWERTZ",
    "Sandro",
    "Sketchfab",
    "UBII",
    "acrolist",
    "breaklines",
    "captionpos",
    "checktex",
    "chktex",
    "darkgray",
    "discretize",
    "escapechar",
    "extendedchars",
    "hbox",
    "hidelinks",
    "hree",
    "keywordstyle",
    "lstinline",
    "lstlistings",
    "mathpazo",
    "morecomment",
    "morestring",
    "ndkeywords",
    "ndkeywordstyle",
    "numbersep",
    "numberstyle",
    "pgfplots",
    "proto",
    "showspaces",
    "showstringspaces",
    "tabsize",
    "textleftarrow"
  ],
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "handlebars",
    "html",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
